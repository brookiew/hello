#!/usr/bin/env node

'use strict'

var c = require('skilstak-colors');

var printHello = function () {
  console.log("Hello Waffles");
}

var printForever = function() {
  while (true){
  console.log(c.rc + "Hello Waffles");
}

var printRed = function() {
  console.log(c.multi("Hello Waffles")
}

var printColored = function(waffles) {
  console.log(c.red + waffles + c.x);

}

printRed("waffles");


